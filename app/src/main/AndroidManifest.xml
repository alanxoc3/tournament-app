<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cs246.fencing_tournament">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="fencing-tournament"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".screens.MainScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".screens.EnterContestant" />
        <activity android:name=".screens.PoolScreen" />

        <activity android:name=".screens.matchScreen"
            android:screenOrientation="landscape"></activity>
    </application>

</manifest>